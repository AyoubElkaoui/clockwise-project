{
  "permissions": {
    "allow": [
      "Bash(ls:*)",
      "Bash(Get-Process -Name dotnet -ErrorAction SilentlyContinue)",
      "Bash(Where-Object { $_Path -like \"*clockwise-project*\" })",
      "Bash(Stop-Process -Force)",
      "Bash(Out-Null)",
      "Bash(Write-Host \"Backend processes stopped \\(if any were running\\)\")",
      "Bash(pkill:*)",
      "Bash(cat:*)",
      "Bash(npm run build:*)",
      "Bash(dotnet build:*)",
      "Bash(npx tsc:*)",
      "Bash(where:*)",
      "Bash(npx next build)",
      "Bash(dir:*)",
      "Bash(git checkout:*)",
      "Bash(node node_modules/.bin/tsc:*)",
      "Bash(findstr:*)",
      "Bash(Select-String \"error\")",
      "Bash(Select-String -NotMatch \"MSB3026|MSB3027|MSB3021|warning\")"
    ]
  }
}
